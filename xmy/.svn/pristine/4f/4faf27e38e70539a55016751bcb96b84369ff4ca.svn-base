header .header-top{border-bottom: 1px solid #C3C3C3;}
header .header-top .pull-right{font-size: 0.7rem;color: #999;}
.content{bottom: 0;}
.state{height: 2.5rem;width: 100%;background-color: #fff;text-align: center;font-size: 0.7rem;color: #999;line-height: 2.5rem;}
.state .active{color: #348811;border-bottom: 2px solid #348811;}
.lump .lump2, .lump .lump3{display: none;}
.lump .status{height: 2.5rem;width: 100%;background-color: #fff;text-align: center;font-size: 0.7rem;color: #999;line-height: 2.5rem;border-top: 1px solid #f0f0f0;}
.lump .status .weui-flex__item{position: relative;}
.lump .status .weui-flex__item img{width: 1rem;height: 1rem;position: relative;top: 0.25rem;margin-left: 0.25rem;}
.lump .status .active{color: #348811;}
.weui-panel{background-color: #f0f0f0;margin-top: 1rem !important;}
.weui-panel .weui-panel__bd{padding: 0 10px;position: relative;}
.weui-panel .weui-panel__bd .weui-media-box{padding: 0;background-color: #fff;margin-bottom: 0.5rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__hd{width: 6rem;height: 5rem;line-height: auto;background: url(../images/yhq.png);background-size: 100% 100%;color: #fff;font-size: 0.9rem;margin-right: 0.5rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__hd .money{margin-top: 0.25rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__hd .money span{font-size: 2rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__hd p{font-size: 0.7rem;line-height: 0.8rem;margin-top: -0.25rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__bd{height: 5rem;position: relative;padding-right: 10px;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__bd .weui-media-box__desc{margin-top: 0.5rem;-webkit-line-clamp:3;line-height: 0.8rem;height: 2.4rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__bd .times{color: #B6B6B6;font-size: 0.6rem;margin-top: 0.2rem;}
.weui-panel .weui-panel__bd .weui-media-box .weui-media-box__bd .times p{line-height: 0.8rem;}
.weui-actionsheet .titled{font-size: 0.7rem;color: #999;text-align: left;padding: 0 10px;}
.weui-actionsheet .weui-actionsheet__cell{padding: 15px 0;}
.weui-actionsheet .bound{background-color: #348811;height: 1.75rem;width: 90%;border:0;color: #fff;font-size: 0.7rem;border-radius: 3px;}
.weui-actionsheet .weui-actionsheet__action{display: none;}
