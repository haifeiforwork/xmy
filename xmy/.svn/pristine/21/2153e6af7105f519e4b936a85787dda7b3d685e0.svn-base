.content{bottom: 0}
/*自定样式*/
.clearfix {  *zoom: 1;  }
.clearfix:before,
.clearfix:after {  display: table;  line-height: 0;  content: "";  }
.clearfix:after {  clear: both;  }
.container-white{background-color: #FFF;width: 100%;}
.container .logo-area img{width: 100%;}
ul{list-style-type: none;padding: 0;margin: 0;}
.container-wrap{padding: 0 0.5rem;}
/*头部中间图片*/
.jx-title{width: 80px;height: 18px;position: relative;top: 0.15rem;}

/*所有大图*/
.banner img{width: 100%;display: block}
.banner{position: relative;}

/*栅格系统*/
.col-69{width: 73%;float: left;}
.col-28{width: 24%;float: left;}

/*商品列表*/
.shop-list .col-33{width: 32.5%;margin-right: 1.24%;float: left;margin-top: 2%;padding:0.2rem 0 0.3rem; }
.shop-list .col-33:nth-of-type(3n){margin-right: 0;}
.shop-list .shop-img{width:100%;display: block;margin: 0 auto}
.shop-list .price{font-size: 0.7rem;color: #F92129;margin-top: 0.2rem;float: left;}

/*列表新增*/
.lq-row{background-color: #FFF;padding:0 0.3rem 0.5rem;position: relative;border-bottom-left-radius: 0.3rem;border-bottom-right-radius: 0.3rem;}
.lq-row .shop-list{padding-top: 0.5rem;padding-bottom: 0.2rem;}
.lq-row .col-33{margin-top: 0;padding: 0;margin-bottom: 0;width: 30%;margin-right: 5%;position: relative}
.lq-row .price{background-color: rgba(0,0,0,0.1);width: 100%;bottom: 0;position: absolute;text-align: center;font-size: 0.6rem;margin-top: 0;color: #FF2621;}

/*优惠券默认样式*/
.lq{margin:0.5rem;color: #FFF;background-color: #FFF;border-radius: 0.3rem;}
.lq .lq-title{background: url("../images/bg-lq-ylq1@2x.png") no-repeat;background-size: 100% 100%;padding: 0.2rem 0.5rem 0.5rem;position: relative;z-index: 66;overflow: hidden}
.lq-finish .lq-title{background: url("../images/bg-lq-ylw@2x.png") no-repeat;background-size: 100% 100%;padding: 0.2rem 0.5rem;position: relative;z-index: 66;overflow: hidden}
.lq .lq-title .lq-price{font-size: 28px;}
.lq .lq-title .one-stence{font-weight: 600;font-size: 0.7rem;display: -webkit-box;  -webkit-box-orient: vertical;  -webkit-line-clamp: 1;  overflow: hidden;line-height: 1.2;}
.lq .lq-title .one-stence .lq-limit{font-weight: normal;}
.lq .lq-title .two-stence{font-size: 0.5rem;}
.lq .lq-title .use-condition{margin-right: 0.2rem;}
.lq .lq-title .last-time{line-height:1;font-size:0.5rem;}
.lq .lq-title .floor{overflow:hidden;background-color: #FF8E01;font-size: 0.6rem;height: 19px;border-radius: 9.5px;position: relative;padding: 1px 2px;margin-top: 0.4rem;margin-bottom: 0.4rem;}
.lq-finish .lq-title .floor{background-color: #A4A2A3;}
.lq .lq-title .floor-pro{text-align: center;width: 69%;height: 15px;border-radius: 7.5px;line-height: 16px;display: inline-block;}
.lq-finish .lq-title .floor-pro{width: 100%;background-color: #FFF;color: #A0A0A0;}
.lq .lq-btn{background: url("../images/lq-btn-bg.png") no-repeat;background-size: 100% 100%;color: #FF8E00;font-size: 0.7rem;text-align: center;padding: 0.1rem 0}
.lq-to-use .lq-btn{background: url("../images/lq-to-use.png") no-repeat;background-size: 100% 100%;margin-top: 1.5rem;}
.lq-finish .lq-btn{background: url("../images/lq-final.png") no-repeat;background-size: 100% 100%;color: #FFF;}
.lq .redy{width: 50px;position: absolute;left: 45%;}
.lq .my-titke{position: absolute;width: 60px;top: -30px;z-index: 66;left: 50%;margin-left: -30px;}


/*我的优惠券*/
.lq-ad-bg{background-color: #358812;}
.lq .lq-title .floor-bar{position:absolute;top:1px;left:0;height:16px;border-radius:9.5px;background-color:#ffc128;z-index:2;width:50%;}
.lq .lq-title .floor-pro{position:relative;z-index:3;}

