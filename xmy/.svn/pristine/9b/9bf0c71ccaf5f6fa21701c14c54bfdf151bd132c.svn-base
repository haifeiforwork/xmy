package com.zfj.xmy.wap.web.common;


public class QQLoginDto {
	
	//回调后返回的code(authorization_code)
	private String code;
	
	//通过code返回的access_token
	private String access_token;
	
	//expires_in(消逝时间、过期时间)
	private String expires_in;
	
	public QQLoginDto() {}

	/**
	 * @param code
	 * @param access_token
	 * @param expires_int
	 */
	public QQLoginDto(String code, String access_token, String expires_in) {
		super();
		this.code = code;
		this.access_token = access_token;
		this.expires_in = expires_in;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getAccess_token() {
		return access_token;
	}

	public void setAccess_token(String access_token) {
		this.access_token = access_token;
	}

	public String getExpires_in() {
		return expires_in;
	}

	public void setExpires_int(String expires_in) {
		this.expires_in = expires_in;
	}
	
}
