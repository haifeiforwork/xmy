@CHARSET "UTF-8";
.content{top: 0;bottom: 0;}
        .head{height: 8rem;background: url(../images/gr-bg.png);background-size: 100% 100%;}
        .head .chunk{text-align: center;color: #fff;font-size: 0.6rem;height: 6rem;position: relative;top: 1rem;}
        .head .chunk .weui-flex__item img{width: 1.3rem;height: 1.3rem;}
        .head .chunk .weui-flex__item p{position: relative;top: -0.1rem;}
        .head .chunk .weui-flex__item.tou{padding: 0 0.75rem;position: relative;}
        .head .chunk .weui-flex__item.tou img{width: 4rem;height: 4rem;border:1px solid #fff;border-radius: 50%;margin-top: 0.25rem;z-index: 99;}
        .head .chunk .weui-flex__item.tou .not-bound{background-color: #656565;border-radius: 0.45rem;z-index: 999;position: absolute;top: 3.85rem;left: 15%;width: 70%;}
        .head .chunk .weui-flex__item.tou .phone{margin-top: 0.3rem;font-size: 0.7rem;}
        .head .chunk .weui-flex__item.lump{margin-top: 1.25rem;height: 2rem;}
        .head .chunk .weui-flex__item.lump .fixation{width: 1.3rem;height: 1.3rem;position: relative;margin: 0 auto 0.3rem auto;}
        .head .chunk .weui-flex__item.lump .fixation .number{width: 0.6rem;height: 0.6rem;border-radius: 50%;background-color: #F42D30;color: #FFF;text-align: center;line-height: 0.6rem;font-size: 0.4rem;position: absolute;top: 0rem;right: 0rem;}
        .gr-message{height: 3rem;text-align: center;border-bottom: 1px solid #F2F2F2;background-color: #fff;font-size: 0.7rem;}
        .gr-message .left{border-right: 1px solid #F2F2F2;}
        .gr-message .weui-flex__item .number{margin-top: 0.25rem;}
        .gr-message .weui-flex__item .title{color: #fff;width: 3rem;margin: 0 auto;border-radius: 0.5rem;}
        .gr-message .weui-flex__item .red{color: #F42D30;}
        .gr-message .weui-flex__item .green{color: #348811;}
        .gr-message .weui-flex__item .red-bg{background-color: #F42D30;}
        .gr-message .weui-flex__item .green-bg{background-color: #348811;}
        .menu{text-align: center;font-size: 0.55rem;color: #999;background-color: #fff;height: 4rem;}
        .menu .weui-flex__item img{width: 1.2rem;height: 1.2rem;}
        .menu .weui-flex__item.pj .fixation{position: relative;width: 1.2rem;height: 1.2rem;margin:0 auto;text-align: center;line-height: 1.2rem;margin-top: 0.9rem;}
        .menu .weui-flex__item.pj .fixation img{margin-top: 0;}
        .menu .weui-flex__item.pj p{margin-top: 0.25rem;}
        .menu .weui-flex__item.pj .fixation .number{width: 0.7rem;height: 0.7rem;border-radius: 78%;background-color: #F42D30;color: #fff;text-align: center;line-height: 0.7rem;font-size: 0.4rem;position: absolute;top: -0.2rem;right: -0.4rem;}
        .area{height: 5rem;background-color: #fff;margin-top: 0.5rem;overflow: hidden;text-align: center;font-size: 0.55rem;color: #999;}
        .area .ly-33{width: 33%;float: left;height: 100%;border-right: 1px solid #F2F2F2;}
        .area .ly-33:last-child{border-right: 0;}
        .area .ly-100{height: 2.5rem;line-height: 2.5rem;border-bottom: 1px solid #F2F2F2;}
        .area .shopping-card{position: relative;}
        .area .shopping-card .hot{width: 2rem;height: 2rem;position: absolute;top: 0rem;right: 0rem;}
        .area .shopping-card .hot img{width: 100%;height: 100%;margin-top: 0;}
        .area .shopping-card img{width: 2rem;height: 2rem;margin-top: 0.9rem;margin-bottom:0.3rem;}
        .area .shopping-card p{position: relative;top: -0.3rem;}
        .area .ly-100 img{width: 1.3rem;height: 1.3rem;margin-right: 0.5rem;position: relative;top: 0.5rem;}
        .area .ly-100.qx{border-bottom: 0;}
        .area .ly-100.zj img{margin-left: -10%;}
        .area .ly-100.bh img{margin-left: 10%;}
        .list .list-title{margin-top: 1rem;}
        .list .list-title img{width: 100%; margin-bottom: 15px;}
        .list .list-ul{overflow: hidden;list-style-type: none;}
        .list .list-ul li{width: 49%;float: left;padding: 10px 10px;background-color: #fff;margin-right: 1%;margin-top: 0.25rem;}
        .list .list-ul li:nth-child(2n){margin-right: 0;margin-left: 1%;}
        .list .list-ul li img{width: 100%;}
        .list .list-ul li .details{margin-top: 0.25rem;color:#333;font-size: 0.7rem;line-height: 0.8rem;overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;height: 1.65rem;}
        .list .list-ul li .money{font-size: 0.75rem;color: #F42D30;}