.content{bottom: 0}
        /*�Զ���ʽ*/
        .clearfix {  *zoom: 1;  }
        .clearfix:before,
        .clearfix:after {  display: table;  line-height: 0;  content: "";  }
        .clearfix:after {  clear: both;  }
        .container-white{background-color: #FFF;width: 100%;/* height: 100%; */}
        .header-top .pull-center{height:50px;}
        .jx-title{width: 70px;height: 18px;position: relative;top: 0.7rem;}

        /*���д�ͼ*/
        .banner img{width: 100%;display: block}
        .banner{position: relative;}
        .banner .more{position: absolute;right: 0.5rem;bottom: 20%;z-index: 55;font-size: 0.6rem;color: #333;}
        .banner .more img{width: 14px;height: 14px;position: relative;top: 1px;left: -2px;display: inline-block;}

        /*դ��*/
        .col-49{width: 48.5%;margin-right: 3%;float: left}
        .row4{margin: 0 0.5rem;}

        /*��Ʒ�б�*/
        .shop-list{}
        .shop-list .col-49{padding: 0.2rem;margin-top: 0.5rem;}
        .shop-list .col-49:nth-of-type(2n){margin-right: 0;}
        .shop-list .col-33{width: 32.5%;margin-right: 1.24%;float: left;margin-top: 2%;padding:0.2rem 0 0.3rem; }
        .shop-list .col-33:nth-of-type(3n){margin-right: 0;}
        .shop-list .shop-img{width:100%;display: block;margin: 0 auto}
        .shop-list .shop-name{font-size: 0.6rem;display: -webkit-box;  -webkit-box-orient: vertical;  -webkit-line-clamp: 2;  overflow: hidden;color: #333;height: 40px;padding: 0 0.2rem;margin-top: 0.1rem;}
        .shop-list .price{font-size: 0.8rem;color: #F92129;margin-top: 0.2rem;float: left;}
        .shop-list .shop-list-foot{text-align: right;padding: 0.1rem 0.2rem;overflow: hidden}
        .shop-list .col-33 .shop-list-foot{margin-top: -0.4rem;}
        /*�б�����*/
        .jx-row .col-33{margin-top: 0;padding: 0;margin-bottom: 0;width: 32.65%;margin-right: 1%}
        /* .jx {margin-bottom: 0.35rem;} */
		.jx  .title-line{position: relative;text-align: center;margin:0.25rem 0;}
		.jx  .title-line .center-des{font-size: 0.9rem;position: relative;font-weight: 600;}
		.jx  .title-line .center-des:after,.jx .title-line .center-des:before{content: "";display: block;height: 2px;width: 40%;position: absolute;background-color: #E3E3E3;top: 10px;}
		.jx  .title-line .center-des:after{right: -53px;}
		.jx  .title-line .center-des:before{left: -53px;}
		.jx  .title-line .center-des img{width: 30px;height: 30px;margin-right: 0.1rem;position: relative;top: 8px;}
		.jx  .title-line .more{position: absolute;right: 0.5rem;font-size: 0.65rem;color: #999;top: 5px;}
		.jx  .title-line .more img{width: 16px;height: 16px;position: relative;top: 3px;}