.clearfix:after{content: "";display: block;clear: both;overflow: hidden;}
.gutter-top{margin-top: 0.2rem;}
 .container-white{background-color: #FFF;} 
.container .logo-area img{width: 100%;}
.container .self-cell{font-size: 0.7rem;}
.container .theme-color{color: #358812;}
.container .text-right{text-align: right;}
ul{list-style-type: none;padding: 0;margin: 0;}
.self-cell .weui-cell{padding: 10px;}
.self-cell .weui-input::-webkit-input-placeholder { /* WebKit browsers */ color:#999;  }
.self-cell .weui-input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color:#999;}
.self-cell .weui-input::-moz-placeholder { /* Mozilla Firefox 19+ */color:#999;}
.self-cell .weui-input:-ms-input-placeholder { /* Internet Explorer 10+ */color:#999;}
.self-cell .hd-img{width: 25px;height: 25px;margin-right: 10px;}
.container-wrap{font-size: 0.7rem;padding: 5px 10px;overflow: hidden}
.self-flex2{font-size: 0.6rem;color:#666;text-align: center;background-color: #FFF;padding: 0.6rem 0 0.3rem;margin-bottom: 0.5rem;margin-top: -0.25rem;overflow-x:auto;white-space:nowrap;}
.self-flex2 .weui-flex__item{display:inline-block;width:24.5%}
.self-flex2 .img img{width: 45px;height: 45px;}
.banner img{width: 100%;}
.banner{position: relative;}
.banner .top-icon{width: 28px;height: 28px;position: absolute;right: 5px;bottom: 2.3rem;}
.header-top2{background: linear-gradient(to right,#51B304,#429C00);color: #FFF;height: 50px;}

.header-top2 .pull-left{width:35%;}
.header-top2 .pull-left img{width: 100%;height: auto}
.header-top2 .pull-center{overflow: hidden;}
.header-top2 .head-phone{font-size: 0.6rem; text-align: right;float: right;position: relative;margin-top: 10px;margin-right: 3.3rem;color: #CCE5BA;}
.header-top2 .head-phone img{width: 26px;height: 26px;position: absolute;left: -28px;top: 2px;}
.header-top2 .head-phone p{line-height: 1.2;}

.container-wrap .more-icon{width: 14px;height: 14px;position: relative;top: 1px;left: -2px;}
.container-wrap .col-80{width: 80%;float: left;}
.container-wrap .col-20{width: 20%;float: left;}
.container-wrap .right-more{margin-top: 0.3rem;color: #686868;text-align: right}
.container-wrap .inlin-block{display: inline-block;margin-top: -0.2rem;}
.container-wrap .time-esc{margin-left: 0.5rem;}
.container-wrap .inlin-block .hours,.container-wrap .inlin-block .miu,.container-wrap .inlin-block .s{background-color: #161616;color: #FFF;border-radius: 0.2rem;width: 22px;height: 21px;text-align: center;display: inline-block;line-height: 21px;}
.container-wrap .name{color: #FF3845;font-size: 0.8rem;}
.container-wrap .name img{width: 27px;height: 27px;position: relative;top:7px;margin-right: 0.2rem;margin-left: -0.2rem;}

.row2{margin: 0.5rem 0 0.2rem;overflow-x:auto;white-space:nowrap;}
.row2 .half-circle{position: absolute;top: 0;left: 0;color: #FFF;font-size:0.5rem;width:35px;height:30px;}
.row2 .date-time{position:absolute;font-size:0.4rem;color:#FFF;top:8px;z-index:777;left:1px;}
.row2 .half-circle:after{content:"";display:block;width:35px;height:32px;position:absolute;left:0;top:4px;background:url("../images/index-shop-bg-red.png") no-repeat;background-size:100% 100%;}
.row2 .half-circle-gray{position: absolute;top: 0;left: 0;color: #FFF;font-size:0.5rem;width:35px;height:30px;}
.row2 .half-circle-gray:after{content:"";display:block;width:35px;height:32px;position:absolute;left:0;top:4px;background:url("../images/index-shop-bg.png") no-repeat;background-size:100% 100%;}
.row2 .col-33{display:inline-block;width: 32%;position: relative;margin-right: 2%}
.row2 .col-33:last-child{margin-right: 0;border-right: 0;}
.row2 .col-33 .shop-img{width:100%;display: block;margin: 0 auto}
.row2 .col-33 .shop-name{font-size: 0.6rem;overflow: hidden;color: #333;    white-space: nowrap;
    text-overflow: ellipsis;}
.row2 .col-33 .price{font-size: 0.6rem;color: #EC4C4E;margin-top: 0.2rem;font-weight: bolder;}
.row2 .col-33 .price1{color: #656565; font-size: 0.6; font-weight: bolder;}
.row2 .col-45{width: 45%;float: left;height: 10rem;}
.row2 .col-55{width: 55%;float: left;height: 10rem;}
.img-lay{background-color: #FFF;border-bottom: 1px solid #f2f2f2;height: 10rem;margin-bottom:3px;}
.img-lay .img-left{height: 10rem;display:block}
.img-lay .img-left, .img-lay .img-right{width: 100%;display: block}
.img-lay .img-right{height: 5rem;}
.row3 .col-33{width: 33%;margin-right: 0;border-right: 1px solid #f2f2f2;background-color: #FFF;text-align: left;margin-left:-4px;}
.row3 .col-33 .shop-img{width: 100%;}
.row3 .col-33 .shop-name{padding: 0 0.4rem; ;height: 38px;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;white-space:initial;text-overflow:initial;}
.row3 .col-33 .price{padding: 0 0.4rem 0.4rem;margin-top: 0;}


.center-des-g{color:#559424;}
.center-des-o{color:#FE8622;}
.center-des-b{color:#027AE6;}
.center-des-r{color:#d00f10;}
.center-des-q{color:#10cddd;}
.center-des-qg{color:#A2CE17;}
.center-des-qr{color:#F74461;}
.list-wrap{margin-bottom: 0.35rem;}
.list-wrap .title-line{position: relative;text-align: center}
.list-wrap .title-line .center-des{font-size: 0.9rem;position: relative;font-weight: 600;}
.list-wrap .title-line .center-des:after,.list-wrap .title-line .center-des:before{content: "";display: block;height: 2px;width: 40%;position: absolute;background-color: #E3E3E3;top: 9px;}
.list-wrap .title-line .center-des:after{right: -53px;}
.list-wrap .title-line .center-des:before{left: -53px;}
.list-wrap .title-line .center-des img{width: 30px;height: 30px;margin-right: 0.1rem;position: relative;top: 8px;}
.list-wrap .title-line .more{position: absolute;right: 0.5rem;font-size: 0.7rem;color: #999;top: 8px;line-height:25px;}
/* .list-wrap .title-line .more{position: absolute;right: 0.5rem;font-size: 0.7rem;color: #999;top: 12px;}
>>>>>>> .r12008 */
.list-wrap .title-line .more img{width: 16px;height: 16px;position: relative;top: 2px;}

.self-flex2::-webkit-scrollbar,.row2::-webkit-scrollbar {
    display: none;
}

  /*2017-11-13*/
.down-app{background: #000000;height: 50px;padding-left: 8px;display: none;}
.down-app div{float: left;}
.down-app .down-app-close img{width: 20px;padding-top: 15px;}
.down-app .down-app-logo img{width: 50px;padding-top: 13px;padding-left:10px;vertical-align: bottom;}
.down-app .down-app-p{color: #FFFFFF;font-size: 0.5rem;padding:7px 10px 0; }
.down-app .down-app-p p:last-child{color: #999999;font-size: 0.3rem;}
.down-app .down-app-a{float: right;height: 100%;}
.down-app .down-app-a a{display: block;width: 100px;height: 100%;line-height: 50px;text-align: center;color: #FFFFFF; background: #348811;}
.header-100{height: 100px;}
.content-100{top:100px;}
.newcomer-packet{width: 100%;height: 100%; background: rgba(0,0,0,0.5);  position: fixed;top: 0;left: 0;z-index: 4000;}
.newcomer-packet .packet-close{width: 100%;text-align: right;}
.newcomer-packet .packet-close img{width: 30px;}
.newcomer-packet .packet-content{width: 100%;padding: 15px; position: absolute;top: 50%;left: 50%;transform: translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}
.newcomer-packet .packet-img{width: 100%;}
.newcomer-packet .packet-img img{max-width: 100%;/*padding: 40px;*/}
.newcomer-packet .packet-btn{text-align: center;padding-top: 20px;}
.newcomer-packet .packet-btn a{font-size: 1rem;padding:10px 20px;border-radius: 4px;}
.newcomer-packet .packet-down-btn{background: #fdf45b;color: #fa2b00;-webkit-box-shadow: 0px 6px 0px #feb204;box-shadow: 0px 6px 0px #feb204;}
.newcomer-packet .packet-look-btn{background: #ec2e2e;color: #FFFFFF;margin-left:5px;-webkit-box-shadow: 0px 6px 0px #c20907;box-shadow: 0px 6px 0px #c20907;}


@media screen and (min-width: 320px) and (max-width: 359px) { .header-top2 .pull-left{width: 25%;height: auto;left: 0.2rem;} }
        @media screen and (min-width: 360px) and (max-width: 374px) { .header-top2 .pull-left{width: 30%;height: auto;left: 0.2rem;} }
        @media screen and (min-width: 375px) and (max-width: 480px) { .header-top2 .pull-left{width: 35%;height: auto;left: 0.2rem;} }
        @media screen and (min-width: 768px) and (max-width: 1024px) { .header-top2 .pull-left{width: 20%;height: auto;left: 0.2rem;} }
.header-top2 .head-phone a{color:#FFF;display:block;line-height:1.4}


 .shop-list.row3{overflow-x: auto;white-space: nowrap;position: relative}
        .shop-list.row3::-webkit-scrollbar{display: none;opacity: 0;width: 0;height: 0;}