package com.zfj.xmy.activity.persistence.cms.pojo.dto;  

import java.util.Date;

import com.zfj.xmy.common.persistence.pojo.LimitGoods;

/** 
 * @Title: LimitGoodsDto.java 
 * @Package com.zfj.xmy.activity.persistence.pojo.dto 
 * @Description: 
 * @author hexw
 * @date 2017年7月6日 下午8:56:17 
