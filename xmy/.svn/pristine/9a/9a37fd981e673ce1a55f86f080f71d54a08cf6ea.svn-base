.seckill .name img{width: 22px;height: 22px;position: relative;top:7px;margin-right: 0.2rem;top: 5.5px;}
.seckill .name img.mjz-icon{width: 17px;height: 17px;top: 2px;}

/*��һ��ÿ����ɱ*/
.self-pannel{margin-top: 0;}
.self-pannel:after, .self-pannel:before{position: static}
.self-pannel .weui-media-box:before{left: 0;}
.self-pannel .weui-media-box{padding: 15px 10px;}
.self-pannel .weui-media-box_appmsg .weui-media-box__hd{width: 100px;height: 100px;}
.self-pannel .tdate{position: absolute;left: 10px;top: 18px;width: 40px;height:45px;background: url("../images/index-shop-bg-red.png") no-repeat;background-size: 100% auto;background-position: left top;color: #FFF;font-size: 0.4rem;text-align:center;}
.self-pannel .tdate.tdate-g{background-image: url("../images/index-shop-bg.png")}
.self-pannel .tdate span{line-height: normal;white-space:nowrap;position:relative;top:9px;}
.self-pannel .weui-media-box__desc{color: #333;height: 31px;margin-bottom: 20px;}
.self-pannel .price{color: #FA222F;font-size: 0.8rem;font-weight: 600;float: left;padding-top: 0.2rem;}

/*��Ʒ�б�*/
.shop-list{}
.shop-item{background-color: #FFF;padding: 0 0 0.2rem;margin-bottom: 0.2rem;}
.shop-list .shop-item:last-child{margin-bottom: 0;}
.shop-list .half-circle{width: 45px;height: 40px;position: absolute;top: 6;left: 9px;color: #FFF;}
.shop-list .half-circle2{width: 45px;height: 40px;position: absolute;top: 6;left: 9px;color: #FFF;}
.shop-list .half-circle img,.shop-list .half-circle2 img{width:100%;height:100%;}
.shop-list .col-49{padding: 0.2rem;margin-top: 0.5rem;position:relative;}
.day-sign{width:40px;position:absolute;top:0.4rem;left:0.4rem;z-index:22}
.day-sign img{width:100%;display:block;}
.shop-list .col-49:nth-of-type(2n){margin-right: 0;}
.shop-list .col-50{position: relative;padding: 0 0.2rem;}
.shop-list .col-50 .shop-name{padding: 0;}
.shop-list .col-33{width: 32.5%;margin-right: 1.24%;float: left;margin-top: 2%;padding:0.2rem 0 0.3rem; }
.shop-list .col-33:nth-of-type(3n){margin-right: 0;}
.shop-list .shop-img{width:100%;display: block;margin: 0 auto}
.shop-list .shop-name{font-size: 0.6rem;display: -webkit-box;  -webkit-box-orient: vertical;  -webkit-line-clamp: 2;  overflow: hidden;color: #333;height: 40px;padding: 0 0.2rem;margin-top: 0.1rem;}
.shop-list .price{font-size: 0.8rem;color: #F92129;margin-top: 0.2rem;float: left;}
.shop-list .shop-list-foot{text-align: right;padding: 0.1rem 0.2rem;}
.shop-list .col-33 .shop-list-foot{margin-top: -0.4rem;}
.shop-list-foot2{margin-top: 0.3rem;text-align: center;margin-bottom: 0.1rem;}
.shop-list-foot2 .prcie{font-size: 0.8rem;color: #F92129;margin-top: 0.2rem;}
.shop-list-foot2 .word-des{color: #6A6A6A;font-size: 0.6rem;padding:0 0.4rem;}