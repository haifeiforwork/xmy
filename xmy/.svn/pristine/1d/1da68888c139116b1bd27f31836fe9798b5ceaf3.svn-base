	@keyframes fadeIn {
		0% {
			opacity: 0;
			top : -50px;
		}
		100% {
			opacity: 1;
			top : 0;
		}
	} 
	@keyframes fadeLeft {
		0% {
			opacity: 0;
            margin: -160px 0 0 -700px;
		}
		100% {
			opacity: 1;
            margin: -160px 0 0 -680px;
		}
	}
.fadein{
		-webkit-animation: fadeIn 0.7s ease both;
		-o-animation: fadeIn 0.7s ease both;
		animation: fadeIn 0.7s ease both;}
.float-icon{
	-webkit-animation: fadeLeft 0.7s ease both;
	-o-animation: fadeLeft 0.7s ease both;
	animation: fadeLeft 0.7s ease both;}
