.table .commodity-table .table-tr .table-td .commodity-list:last-child{border-bottom: 0;}
.table .commodity-table .table-tr .money{text-align: center;font-size: 13px;color:#333;border-left: 1px solid #E5E5E5;margin-top: 0;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.table .commodity-table .table-tr .money .number{margin-top: 15px;}
.table .commodity-table .table-tr .money .units{font-size: 13px;color: #333;margin-top: 5px;}
