.table .table-fy .table-fy-right .paging-ul li.active{background-color: #258912;color: #fff;border:1px solid #258912;}
.table .table-fy .table-fy-right .paging-ul .even{border:0;}
.table .table-fy .table-fy-right span{font-size: 12px;color: #333;border:1px solid #D7D7D7;padding:1px 5px;border-radius: 3px;margin-left: 5px;cursor: pointer;}
.table .table-fy .table-fy-right span.active{color:#999;}
