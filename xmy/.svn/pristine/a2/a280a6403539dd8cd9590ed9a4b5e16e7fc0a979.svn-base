.add-shop-cart{position: fixed;bottom: 4rem;right: 10px;z-index: 999;width: 2rem;height: 2rem;text-align: center;border-radius: 50%;background-color: rgba(253,68,66,0.9);}
.add-shop-cart img{width: 1rem;height: 1rem;margin-top: 0.5rem;}
header .header-top .pull-left .return{width: 30px;height: 30px;border-radius: 50%;background-color:rgba(0,0,0,0.5);text-align: center;margin-top: 12.5px;margin-left: 5px;position: relative;}
header .header-top .pull-left .return img{position: absolute;top: 2.5px;left: 0px;margin-top: 0;}
header .header-top .pull-right .shrink{background-color:rgba(0,0,0,0.5);display: block;width: 30px;height: 30px;border-radius: 50%;margin-top: 12.5px;position: relative;float: left;margin-left: 0.5rem;}
header .header-top .pull-right .shrink img{position: absolute;top: 2.5px;left: 2.5px;margin-top: 0;}
.gw_num{border: 1px solid #dbdbdb;width: 86px;line-height: 23px;overflow: hidden;height: 23px;}
.gw_num em{display: block;height: 23px;width: 23px;float: left;color: #7A7979;border-right: 1px solid #dbdbdb;text-align: center;cursor: pointer;background: #F0F0F0;line-height: 20px;}
.gw_num .num{display: block;float: left;text-align: center;width: 38px;font-style: normal;font-size: 14px;height: 22px;border: 0;line-height: 22px;}
.gw_num em.add{float: right;border-right: 0;border-left: 1px solid #dbdbdb;}
em{font-style:normal}
.content{top: 0px;}
.flex-direction-nav{display: none;}
.flex-control-nav{bottom: 0;}
.flex-control-paging li a{background-color: #fff;width: 8px;height: 8px;}
.flex-control-paging li a.flex-active{background-color: #348811;}
.flexslider{margin-bottom: 0;}
.head-top{background-color: #fff;padding: 1rem 10px 0.5rem 10px;}
.head-top .title{font-size: 0.7rem;color: #444;}
.head-top .money{font-size: 1.1rem;color: #FD4442;position: relative;}
.head-top .money #activityPrice{font-weight:bold;}
.head-top .money .count{float: right;margin-top: 0.35rem;}
.head-top .line{font-size: 0.6rem;color: #666;margin-top: 0.25rem;}
.head-top .line .right{float: right;}
.head-top .line .right.collect.active{color: #348811;}
.head-top .line .right img{width: 1rem;height: 1rem;float: left;position: relative;top: -0.05rem;}
.head-top .line .green{color: #348811;}
.recommend{background-color: #fff;margin-top: 0.25rem;}
.recommend .title{padding: 10px 10px;font-size: 0.7rem;color: #348811;}
.recommend .list-ul{list-style-type: none;overflow: hidden;}
.recommend .list-ul li{width: 33.3%;float: left;padding: 10px 10px;}
.recommend .list-ul li img{width: 75%;margin-left: 12.5%;}
.recommend .list-ul li .titled{font-size: 0.6rem;color: #444;overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;line-height: 0.8rem;height: 1.6rem;}
.recommend .list-ul li .money{font-size: 0.6rem;color: #FD4442;}
.message{background-color: #fff;margin-top: 0.25rem;}
.message .norms{padding: 12px 10px;overflow: hidden;border-bottom: 1px solid #F7F7F7;}
.message .norms .ly-20{width: 20%;float: left;font-size: 0.7rem;color: #444;}
.message .norms .ly-80{width: 80%;float: left;text-align: center;}
.message .norms .ly-80 .chunk-ul{overflow: hidden;list-style-type: none;}
.message .norms .ly-80 .chunk-ul .chunk-li{font-size: 0.45rem;color: #666;border:1px solid #BBBBBB;height: 1.2rem;line-height: 1.2rem;float: left;margin-right: 0.5rem;border-radius: 2px;margin-bottom: 0.25rem;padding:0 0.2rem;font-size:0.6rem}
.message .norms .ly-80.lump{padding-right: 40px;position: relative;}
.message .norms .ly-80.lump .down{width: 20px;height: 20px;position: absolute;top: 0rem;right: 0rem;}
.message .norms .ly-80.lump .down img{width: 100%;height: 100%;}
.message .norms .ly-80.lump .carry{font-size: 0.6rem;color: #444;text-align: left;}
.message .norms .ly-80.lump .carry span{margin-right: 0.5rem;}
.message .norms .ly-80 .chunk-ul .chunk-li.active{background-color: #348811;color: #fff;border:0;}
.message .list{margin:0;}
.message .list .weui-cell{border-bottom: 1px solid #F7F7F7;font-size: 0.7rem;color: #444;padding: 10px 10px;}
.message .list .weui-cell .weui-cell__bd{font-size: 0.7rem;color: #444;}
.message .list .weui-cell .weui-cell__ft{font-size: 0.7rem;color: #444;}
.message .list .weui-cell .weui-cell__ft span{color: #FD4344;}
.weui-cells:after, .weui-cells:before, .weui-cell:before{display: none;}
footer{border-top: 1px solid #F4F4F4;}
       /* 2017-11-2*/
        #imgword-info{z-index: 1000;}
        #imgword-info .weui-popup__modal{background-color: #fff;position: relative}
        #imgword-info .weui-cell{font-size: 0.75rem;}
        #imgword-info .modal-bd{width: 100%;height: 100%;position: absolute;overflow-y: auto;top:50px;overflow-x: hidden;}
          #imgword-info .modal-bd{-webkit-overflow-scrolling: touch}
        #imgword-info .txtarea{font-size: 0.7rem;float: none;padding-top: 0.2rem;border-top: 1px solid #ddd;position:relative;height:auto;}
        #imgword-info .txtarea img{max-width: 100%;display: block;margin: 0rem auto;float: none;position:relative;}
        #imgword-info .qx-line{position: fixed;bottom: 0;width: 100%;height: 30px;text-align: center;}
        #imgword-info .qx-line img{width: 25px;}
.banner img{width: 100%;}
.banner{position: relative;}

.finish-time{color: #FD4442;font-size: 0.7rem;text-align: center;padding: 0.1rem;}
.finish-time img{width: 20px;margin-right: 0.25rem;position: relative;top: 0.2rem;}

