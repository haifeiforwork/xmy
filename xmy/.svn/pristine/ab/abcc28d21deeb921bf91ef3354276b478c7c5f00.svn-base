.category *{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.category .text-center{text-align: center;}
.category .price{color: #EF1722;}

/*上方搜索*/
.navigation .navigation-left .left-chunk2 .logo-near{color: #FFF;margin-top: 15px;}
.navigation .navigation-right .L-search{margin-right: 0;width: 338px;}
.navigation .navigation-right .L-search input{width: 268px;}
.navigation .navigation-right .L-search button{float: right;color:#258912;}

/*收货人信息*/
.category .balances-title{font-size: 19px;color: #585757;margin: 25px 0 15px;position:relative;}
.category .balances-wrap{border: 1px solid #b5b5b5;padding: 30px;background-color: #FFF;margin-bottom: 15px;}
.category .balances-wrap2{border: none;padding: 0;background-color: #EFEFEF;}
/*购物车表格*/
.category .balances-wrap .shoped{color: #2E2E2E;font-size: 14px;width: 100%;border-spacing: 0;}
.category .balances-wrap .shoped .shoped-con{height: 120px;}
.category .balances-wrap .shoped .shoped-con td{border-bottom: 1px dashed #328E0E;}
.category .balances-wrap .shoped .table-head .shoped-info{padding-left: 40px;width: 330px;}
.category .balances-wrap .shoped .shoped-info2{padding-left: 20px;}
.category .balances-wrap .shoped .shoped-info2 .img-wrapa{width: 90px;float: left;border: 1px solid #a4a4a4;padding: 4px;height: 90px;}
.category .balances-wrap .shoped .shoped-info2 .img-wrapa img{width: 100%;height: 100%;}
.category .balances-wrap .shoped .shoped-info2 .info-des{width: 65%;float: right}
.category .balances-wrap .shoped .shoped-info2 .info-des .price{margin-top: 35px;margin-left: 0;}
.category .balances-wrap .shoped .shoped-con .price{font-weight: 600;}

.category .balances-wrap .shop-car{background-color: #FFF;}
.category .balances-wrap .shop-car .table-head{height: 36px;}
.category .balances-wrap .shop-car tr td:first-child{border-left: 1px solid #B5B5B5;}
.category .balances-wrap .shop-car tr td:last-child{border-right: 1px solid #B5B5B5;}
.category .balances-wrap .shop-car tr:last-child td{border-bottom: 1px solid #B5B5B5;}
.category .balances-wrap .shop-car .shoped-con td{border-bottom: 0;}
.category .balances-wrap .shop-car td{border-top: 1px solid #B5B5B5;}
.category .balances-wrap .shop-car .table-head .shoped-info{padding-left: 0;}
.category .balances-wrap .shop-car .table-head .shoped-info span{margin-right: 25px;}
.category .balances-wrap .shop-car .shoped-info2{padding-left: 0;}
.category .balances-wrap .shop-car .shoped-con .option{padding-left: 55px;width: 140px;}
.category .balances-wrap .shop-car .crease, .category .balances-wrap .shop-car .add{width: 20px;height: 20.5px;background-color: #f0f0f0;font-size: 12px;border:1px solid #DEDEDE;position:relative;z-index:33;}
.category .balances-wrap .shop-car .nuum{width: 29px;text-align: center;height: 20.5px;border: 1px solid ;border-color: #ddd transparent #ddd transparent;outline: 0;position:relative;}
.category .balances-wrap .shop-car .crease{margin-right: -5px;}
.category .balances-wrap .shop-car .add{margin-left: -5px;}
.category .balances-wrap .shop-car .c-price{color: #515151;position: relative;text-align:left;padding-left:50px;}
.category .balances-wrap .shop-car .c-price .c-btn{cursor:pointer;display:inline-block;border: 1px solid #E60116;z-index: 33;background-color: #FFF3EA;color: #EA2A39;padding: 0 1px;position: relative;outline: 0;top:1px;}
.category .balances-wrap .shop-car .c-price .c-btn i{color: #EA303B;position: relative;top: 1px;}
.category .balances-wrap .shop-car .c-price .c-price-des{text-align: left;padding: 5px;line-height: 35px;width:auto;position: absolute;left:50px;;top: 20px;border: 1px solid #EA2A39;display: none;white-space:nowrap;}
.category .balances-wrap .shop-car .active-c .c-btn{border-color: #EA2A39 #EA2A39 transparent #EA2A39;background-color: #FFF;}
.category .balances-wrap .shop-car .table-item{position: relative;top: -25px;}
.category .balances-wrap .shop-car .table-item input[type="checkbox"]{top: -15px;position: relative;z-index: 66;}
.category .balances-wrap .shop-car .one-price{width: 167px;}
.category .balances-wrap .shop-car .one-area{width: 131px;}

/*失效*/
.category .balances-wrap .shop-car .shop-info3{width: 330px;}
.category .balances-wrap .shop-car .shoped-con .none-use{width: 50px;color: #636363;}
.category .balances-wrap .shop-car .shoped-con .none-use span{background-color: #DCDCDC;padding: 0 3px;border-radius: 3px;}
.category .balances-wrap .none-use-all{background-color: #EFEFEF;margin-top: 20px;}
.category .balances-wrap .none-use-all{color: #CACACA;}
.category .balances-wrap .none-use-all .shoped-con .price{color: #CACACA;}
.category .balances-wrap .none-use-all .shoped-con .option{color: #2E2E2E;}
.category .balances-wrap .none-use-all .nuum{}
/*结算部分*/
.table-all{height: 48px;line-height: 48px;font-size: 14px;color: #3b3b3b;background-color: #E5E5E5;margin:25px 0 47px;}
.table-all .table-all-list{display: inline-block;width: 50%;}
.table-all .table-all-list li{display: inline-block;margin-right:30px; position: relative;top: -2px;}
.table-all .table-all-list .che-wrap input{position: relative;top: 1px;margin-right: 5px;margin-left: 5px;}
.table-all .table-all-list2{display: inline-block;width: 50%;text-align: right;margin-left: -5px;}
.table-all .table-all-list2 .redy{display: inline-block;position: relative;top: -2px;}
.table-all .table-all-list2 .redy .price{font-weight: 600;}
.table-all .table-all-list2 .redy .lg-price{font-size: 20px;line-height: 42px;position: relative;top: 2px;margin-left: 8px;}
.table-all .table-all-list2 .redy-choose{margin-right: 25px;}
.table-all .table-all-list2 .redy-choose i{font-size: 12px;color: #999;}
.table-all .table-all-list2 .balances-all{cursor: pointer;height: 48px;padding: 0 40px;border: none;background-color: #B0B0B0;color: #FFF;font-size: 20px;}
/*删除*/
.delete{cursor: pointer;}
.bookMark{cursor: pointer;}
/*20179191142*/
.category .balances-title span.get-tiket-btn{cursor: pointer;color:#FFF;font-size:14px;margin-left: 30px;display: inline-block;padding: 4px 5px;background: url("../../commons/images/002.png")no-repeat;background-size: 100% 100%;margin-bottom: 10px;}
.get-tiket{position: absolute;left: 0;top: 39px;z-index: 333;width: 600px;height: 300px;border: 1px solid #C4424E;background-color: #FFF;padding: 15px;}
.get-tiket .em2{position: absolute;width: 15px;top: -17px;z-index: 666;left: 145px;}
.get-tiket .em2 img{width: 100%;}
.get-tiket-title{padding-bottom: 15px;border-bottom: 1px solid #DDD;}
.get-tiket-close{float: right;font-size: 16px;font-weight: 600;cursor: pointer;}
.get-tiket-close i{color: #C9C9C9;}
.get-tiket-content{overflow-y: scroll;height: 230px;}
.get-tiket-content .item-title{font-size: 13px;color: #333;}
.get-tiket-content .item-after{font-size: 12px;color: #999;}
.get-tiket-content .titket-money-wrap{width: 60px;font-size: 12px;position:relative;top:10px;height:35px;font-size:16px;}
.get-tiket-content .titket-money-wrap .titke-money{position: relative;color: #FFF;background: url("../../commons/images/001.png") no-repeat;background-size: 100% 100%;padding: 7px 2px;display: inline-block;text-align: center;width:100%;}
.get-tiket-content .titket-money-wrap .titke-money .big-size{font-size: 14px;font-weight: 600;}
.get-tiket-content .titket-area{width: 230px;}
.get-tiket-content .titket-area p{margin-bottom: 4px;}
.get-tiket-content .titket-img .img-wrap{display: inline-block;width: 45px;height: 45px;font-size: 13px;color: #666;padding: 5px;text-align: center;border: 1px solid #ddd;cursor: pointer;margin-left:-1px;}
.get-tiket-content .titket-img .img-wrap img{width: 100%;height: 100%;display: block;}
.get-tiket-content .titket-img .img-wrap:last-child{position: relative;top: -8px;}
.get-tiket-content .titket-area-use{width: 146px;cursor: pointer}
.get-tiket-content .titket-area-use i{color: #999;}
.get-tiket-content .titket-area-use .item-title{position: relative;top:5px; }
.get-tiket-content .titket-area-use .titket-img{margin-top: 5px; }
.get-tiket-content .titket-get-btn{}
.get-tiket-content .titket-get-btn .btn{color: #FFF;background-color: #FA0408;font-size: 13px;padding: 3px 10px;border: none;border-radius: 3px;outline: 0;cursor: pointer;position: relative;top: 5px;}
.get-tiket-line{margin: 10px 0;}
.get-tiket-line>div{float: left;margin-left: 15px;position: relative}
/*20179191142end*/
