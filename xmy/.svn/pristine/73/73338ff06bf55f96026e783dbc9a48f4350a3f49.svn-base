.iconfont {
  font-family:"iconfont" !important;
  font-size:14px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
*{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;-webkit-tap-highlight-color: rgba(200,200,200,0);}
body{overflow: hidden;background-color: #F0F0F0;} 
header{height: 50px;position: absolute;top: 0rem;left: 0rem;width: 100%;z-index:999;}
.content{position: absolute;top: 50px;bottom:55px;left: 0rem;width: 100%;z-index:99;overflow: auto;-webkit-overflow-scrolling : touch;background-color: #F0F0F0;}
footer{width: 100%;height: 55px;position: absolute;bottom: 0rem;left: 0rem;z-index:999;background-color: #FAFAFA;}
footer .weui-tabbar{height: 55px;}
.header-top{padding: 0 0.5rem;line-height: 50px;position: relative;background-color: #fff;color: #464646;}
.header-top .pull-left{position: absolute;top: 0rem;left: 0.1rem;height: 50px;}
.header-top .pull-left img{width: 25px;height: 25px;margin-top: 12.5px;}
.header-top .pull-center{text-align: center;font-size: 0.9rem;}
.header-top .pull-right{position: absolute;top: 0rem;right: 0.4rem;}
.header-top .pull-right img{width: 25px;height: 25px;margin-top: 12.5px;}
footer .weui-flex{text-align: center;height: 100%;font-size: 0.6rem;color: #999;background-color: #fff;}
footer .weui-flex .img{width: 100%;height: 25px;margin-top: 7px;position:relative;}
footer .weui-flex .img img{width: 25px;height: 25px;}
footer .weui-flex p{position: relative;top: 0px;}
footer .weui-flex .active p{color: #348811;}
footer .weui-badge{width:14px;height:14px;border-radius:7px;padding:0;}
.weui-dialog__btn{font-size: 0.7rem;color: #444;}

.swiper-slide, .swiper-wrapper{height:auto}
.swiper-slide img{width:100%;display:block}

.shrink{position: relative;}
.modal-some{display: none;position: absolute;right: 0;top: 20px;background-color: rgba(0,0,0,0.6);color: #FFF;font-size: 13px;padding: 0 0.3rem;z-index: 6666;width: 100px;border-radius: 0.2rem;}
.modal-some li{padding: 0.3rem 0;text-align: center;line-height: normal;list-style-type: none;}
.modal-some li{border-bottom: 1px solid #fff;}
.modal-some li:last-child{border-bottom: none;}
.modal-some li img{width: 20px!important;display: inline-block;margin-top: 0!important;height: 20px!important;margin-right: 0.3rem;position: relative;top: 0.15rem;}
