.dNav{height: 46px;background-color: #fff;}
.dNav .list{width: 1250px;height: 100%;margin: 0 auto;min-width: 1250px;}
.dNav .list .list-ul{list-style-type: none;background-color: #fff;height: 100%;line-height: 45px;position: relative;}
.dNav .list .list-ul .rr{width: 122px;height: 63px;position: absolute;top: -20px;right: 90px;}
.dNav .list .list-ul .rr img{width: 100%;height: 100%;}
.dNav .list .list-ul .list-li{float: left;font-size: 16px;}
.dNav .list .list-ul .front{background-color: #358912;color: #fff;width: 150px;position: relative;padding:0 20px;}
.dNav .list .list-ul .front .all i{font-size: 26px;
    position: absolute;
    top: 0px;
    right: 0px;}
.dNav .list .list-ul .front .classify-ul{z-index: 9999;background-color: #3EA600;position: absolute;top: 45px;left: 0px;width: 100%;font-size: 13px;}
.dNav .list .list-ul .out-cross{color: #FCFE14;}

/* 新增头部样式*/
nav .outer .header .second-tell img{margin-top: 10px;}

/*新增导航*/
.add-bg-theam{background-color: #358812!important;color: #FFF;}
.dNav .list .list-ul .type{color: #fff;width: 190px;position: relative;padding: 0;height: 45px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;}
.dNav .list .list-ul .type .classify-ul{z-index: 9999;background-color: #9EE22B;top: 50px;height: 573px;}
.dNav .list .list-ul a{text-decoration: none;color: #525355;}
.dNav .list .list-ul .type-item{width:100%;position: absolute;z-index: 99999;text-align: center;color: #FFF;}
.dNav .list .list-ul .type>img{width: 100%;position: absolute;z-index: 88888;height: 55px;}
.type .type-item-child{padding: 0 15px;color: #525355;margin-bottom:10px; }

.dNav .list .list-ul .type .type-item-child .item-grand{height: auto;line-height: normal;}
.dNav .list .list-ul .type .type-item-child>h4{margin: 0;color: #000;font-size: 14px;}
.dNav .list .list-ul .type .type-item-child a{display: inline-block;margin:0 4px 14px 0;}

/*自写复用样式*/
.gutter{padding: 0 94px;}
.gutter-mar{margin: 0 94px;}
.clearfix:before,.clearfix:after {content: ".";display: block;height: 0;visibility: hidden;}
.clearfix:after {clear: both;}
.clearfix {zoom: 1;}/*兼容IE6/7*/
.col-50{width: 50%;}
div[class^="col"]{float: left;}
.price{color: #E23533;}
.add-bg-w{background-color: #FFF;}
.add-bg-w.sec-control{background-color:transparent;}
.text-center{text-align: center;}
.add-g-b{padding-top: 25px;}
/*内容部分*/
.list-content{width: 1250px;margin: 0 auto;margin-bottom:25px;}
.list-content .list-title{padding: 5px 2px;border-bottom: 2px solid #333;margin: 0;}
.list-content .ct1{border-left:5px solid #358912;color: #358912;margin-bottom:25px;}
.list-content .ct1-line{height: 2px;background-color: #359812;width: 82%;position: absolute;right: 0;top: 35px;}
.list-content .count-title{position: relative;padding: 2px 0;}
.list-content .count-title span{display: inline-block;}
.list-content .count-title span.title-name{font-size: 25px;margin-left: 15px; position:relative;top:2px;}
.list-content .count-title span.ad-word-g{font-size: 25px;margin-left: 5px;font-family: STKaiti;position: relative;top: 3px;}

/*2017-08-21*/
.list-content .first-show img{height: 100%;display: block;width:100%;}
.list-content .list-item{width: 15.2%;border-right: 1px solid #dddddd;height:232px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;float: left;padding:0 10px;}
.list-content .list-item:nth-of-type(7n){border-right:0;}
.list-content .list-item:nth-of-type(12n){border-right:0;}
.list-content .list-item .list-show-wrap{width:76.5%;margin:0 auto;} 
 
.list-content .list-item .list-show img{width: 100%;}
.list-content .list-item .list-show img.gcar{width:auto;margin: 0;line-height: 1;float: right;position: relative;top: 3px;}
.list-content .first-show{width: 24%;float: left;padding-left: 0;border: none;padding-right:0;height:464px;}
.list-content p.text-elips{width: 100%;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;font-size: 14px;}
.list-content .price-dex{height: 40px;line-height: 40px;font-weight: 600;}

/* 鲜一下*/
.banner{background-color: #E7FAC9;position: relative;min-width: 1250px;width: 100%;height: 250px;}
.banner .ban-img{width: 100%;position: absolute;bottom: -34px;}
.banner .banner-des{background-color: #FFF;width: 884px;position: relative;margin-right:149px;margin-top: 15px;border-radius: 5px; float: right;}
.banner .banner-des .img-wrap{width: 80%;position: relative;margin: 0 auto;}
.banner .banner-des .img-wrap img{width: 100%;}
.banner .banner-des .img-wrap span.price{background-color: #DEDEDE;display: inline-block;position: absolute;right: 0;bottom: 0;font-size: 16px;padding: 0 13px;}
.banner .banner-des ul{padding: 15px 20px; }
.banner .banner-des li{width: 25%;float: left;font-size: 14px;color: #525355;}
.banner .banner-des li p{width: 100%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;margin-top: 5px;}
.banner .banner-des .btn-x{width: 90px;position: absolute;right: -90px;top: 10px;}
.banner .banner-des .btn-x img{width: 100%;}
.banner .banner-des .btn-x a{position: absolute;display: inline-block;top: 5px;width: 75px;height: 75px;border-radius: 37.5px;left: 12.5px;}

/*右侧列表图片*/
.ad-h{height: 341px;}
.ad .list-img{width: 1250px;margin: 0 auto;text-align: right;position: relative;z-index: 155555;padding: 10px 0;}
.ad .list-img li a{display: inline-block;}
.ad .list-img li img{width: 205px;}

.list-content .list-item .list-show img:first-child{display:block;width:100%;transition: all 0.3s linear;-moz-transition: all 0.3s linear; /* Firefox 4 */-webkit-transition: all 0.3s linear; /* Safari 和 Chrome */-o-transition: all 0.3s linear; /* Opera */}
.list-content .list-item .list-show img:first-child:hover{transform:translate(-5px,0);-ms-transform:translate(-10px,0); 	/* IE 9 */-moz-transform:translate(-10px,0); 	/* Firefox */-webkit-transform:translate(-10px,0); /* Safari 和 Chrome */-o-transform:translate(-10px,0); 	/* Opera */}
.list-content .first-show img{transition: all 0.3s linear;-moz-transition: all 0.3s linear; /* Firefox 4 */-webkit-transition: all 0.3s linear; /* Safari 和 Chrome */-o-transition: all 0.3s linear; /* Opera */}
.list-content .first-show img:hover{transform:translate(-5px,0);-ms-transform:translate(-10px,0); 	/* IE 9 */-moz-transform:translate(-10px,0); 	/* Firefox */-webkit-transform:translate(-10px,0); /* Safari 和 Chrome */-o-transform:translate(-10px,0); 	/* Opera */}