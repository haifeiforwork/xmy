	/*内容部分*/
.tab-nav{background-color: #FFF;}
.tab-nav ul{border-bottom:1px solid #DDDDDD;padding: 20px 15px 0;text-align: center}
.tab-nav ul li{display: inline-block;width: 13%;border: 1px solid;border-color: #DDDDDD #DDDDDD transparent #DDDDDD ;height: 38px;line-height: 38px;background-color: #F8F8F8;margin-right: 0.5%;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;color: #525355;cursor: pointer;}
.tab-nav ul li:last-child{margin-right: 0;}
.tab-nav ul li.active{position: relative;z-index: 333;background-color: #358812;top: 0.5px;color: #FFF;}
.tab-content{background-color: #358812;padding: 15px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.tab-content .wrap-point{float: left;margin-right: 15px;}
.tab-content .wrap-point:last-child{margin-right: 0;}
.tab-content .wrap-point .point-price:last-child{margin-bottom: 0;}
.tab-content .wrap-point .point1{background-image: url("../../commons/images/shangpin.png")}
.tab-content .wrap-point .point2{background-image: url("../../commons/images/010204001002-001.jpg")}
.tab-content .wrap-point .point4{background-image: url("../../commons/images/010204001002-001.jpg")}
.tab-content .wrap-point .point5{background-image: url("../../commons/images/010204001002-001.jpg")}
.tab-content .wrap-point .point3{background-image: url("../../commons/images/010206000701-005.jpg")}
.tab-content .wrap-point .point6{background-image: url("../../commons/images/010206000701-005.jpg")}
.tab-content .point-price{margin-bottom: 15px;height: 164px;width: 273px;padding: 10px;background: no-repeat;background-position: bottom right;background-color: #FFF;font-size: 15px;}
.tab-content .point-price .point-name{width:100%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap}
.tab-content .point-price .point-des{width: 60%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap}
.tab-content .point-price .price{color: #FE0200;font-size: 17px;font-weight: 600;margin-top: 15px;}
.tab-content .point-price .price span{font-size: 21px;}
.tab-content .lg-point-price{height: 363px;background-position: center bottom;}
.activity-img{overflow: auto;background-color: #F5F5F5;padding: 30px 0;}
.activity-img .chunk{float: left;width: 249px;height: 362px;border-right:1px solid #E5E5E5;}
.activity-img .chunk img{width: 249px;height: 362px;}
.activity-img .chunk .top{width: 249px;height: 180px;border-right:1px solid #E5E5E5;border-bottom:1px solid #E5E5E5;}
.activity-img .chunk .top img{width: 249px;height: 180px;}
.classify{background-color: #EFEFEF;height: 483px;padding: 12px 0px 12px 0px;}
.classify .sing{height: 30px;line-height: 30px;width: 100%;}
.classify .sing span{float: right;font-size: 14px;color: #666666;cursor: pointer;}
.classify .sing span i{font-size: 20px;position: relative;top: 3px;}
.classify .chunk{width: 100%;height: 453px;}
.classify .chunk-d{width: 230px;height: 453px;float: left;}
.classify .fenlei1{background: url(../../commons/images/fenlei1.png);background-size: 100% 100%;}
.classify .fenlei2{background: url(../../commons/images/fenlei2.png);background-size: 100% 100%;}
.classify .fenlei3{background: url(../../commons/images/fenlei3.png);background-size: 100% 100%;}
.classify .fenlei4{background: url(../../commons/images/fenlei4.png);background-size: 100% 100%;}
.classify .fenlei5{background: url(../../commons/images/fenlei5.png);background-size: 100% 100%;}
.classify .fenlei6{background: url(../../commons/images/fenlei6.png);background-size: 100% 100%;}
.classify .fenlei7{background: url(../../commons/images/fenlei7.png);background-size: 100% 100%;}
.classify .chunk-d .chunk-d-ul{overflow: hidden;margin-top: 10px;}
.classify .chunk-d .chunk-d-ul .chunk-d-li{background-color: #fff;width: 55px;height: 55px;font-size: 12px;color: #494A4A;text-align: center;line-height: 55px;border-radius: 50%;float: left;margin-left: 17px;margin-top: 10px;font-weight: 600;}
.classify .chunk-d .chunk-d-ul .red{color: #e4393c;}
.classify .chunk-z{width: 850px;height: 453px;float: left;}
.classify .chunk-z .chunk-z-ul{overflow: hidden;}
.classify .chunk-z .chunk-z-ul .chunk-z-li{width: 169px;height: 226px;float: left;background-color: #fff;border-right: 1px solid #E5E5E5;cursor: pointer;}
.classify .chunk-z .chunk-z-ul .chunk-z-li:nth-child(6), .classify .chunk-z .chunk-z-ul .chunk-z-li:nth-child(7), .classify .chunk-z .chunk-z-ul .chunk-z-li:nth-child(8), .classify .chunk-z .chunk-z-ul .chunk-z-li:nth-child(9), .classify .chunk-z .chunk-z-ul .chunk-z-li:nth-child(10){border-top: 1px solid #E5E5E5;}
.classify .chunk-z .chunk-z-ul .chunk-z-li .img{width: 160px;height: 160px;margin: 0 auto;position:relative;}
.classify .chunk-z .chunk-z-ul .chunk-z-li .img img{width: 100%;height: 100%;position: absolute;top: 0px;left: 0px;}
.classify .chunk-z .chunk-z-ul .chunk-z-li .del{padding: 0 10px;font-size: 12px;color: #666;margin-top: 5px;-webkit-transform-origin-x: 0;-webkit-transform: scale(0.95);overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.classify .chunk-z .chunk-z-ul .chunk-z-li .money{color: #e4393c;font-size: 16px;font-weight: 700;padding: 0 10px;margin-top: 3px;}
.classify .chunk-x{width: 170px;height: 453px;float: left;position: relative;top: -2px;}
.classify .chunk-x .chunk-x-top{width: 170px;height: 226px;}
.classify .chunk-x .chunk-x-top img{width: 100%;height: 100%;}
.lump{overflow: hidden;margin: 20px auto;}
.lump .lump-left, .lump .lump-right{width: 615px;float: left;height: 450px;position: relative;}
