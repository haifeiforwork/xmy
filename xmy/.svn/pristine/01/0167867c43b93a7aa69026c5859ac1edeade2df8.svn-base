.lump .lump-left .content .list-ul .list-li .img{width: 144px;height: 160px;position:relative;text-align: center;}
.lump .lump-right .content .list-ul .list-li .img{width: 144px;height: 160px;position: relative;text-align: center;}
.lump .content .list-ul .list-li .img img{width: 134px;position: absolute;top: 0px;left: 0px;margin-left: 5px;}
.lump .content .list-ul .list-li .del{font-size: 12px;color: #666;padding: 0 10px;-webkit-transform-origin-x: 0;-webkit-transform: scale(0.95);overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.lump .content .list-ul .list-li .money{color: #e4393c;font-size: 16px;font-weight: 700;padding: 0 10px;}
.lump .exchange{width: 625px;float: left;}
