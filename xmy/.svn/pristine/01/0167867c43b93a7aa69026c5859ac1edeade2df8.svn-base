.lump .lump-right .chunk-d-ul .red{color: #e4393c;}
.classify .chunk-d .chunk-d-ul .chunk-d-li{background-color: #fff;width: 55px;height: 55px;font-size: 12px;color: #494A4A;text-align: center;line-height: 55px;border-radius: 50%;float: left;margin-left: 17px;margin-top: 10px;font-weight: 600;}
