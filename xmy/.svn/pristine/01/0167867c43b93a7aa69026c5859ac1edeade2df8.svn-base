.lump .lump-left .content, .lump .lump-right .content{width: 580px;height: 225px;background-color: #fff;position: absolute;bottom: -25px;left: 17.5px;}
.lump .lump-left .more, .lump .lump-right .more{background-color: #2D2E2E;color: #989898;padding: 3px 6px;font-size: 14px;border-radius: 15px;margin-left: 25px;position: relative;top: 75px;}
.lump .lump-left .content .list-ul, .lump .lump-right .content .list-ul{overflow: hidden;height: 100%;}
