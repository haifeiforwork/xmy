        /*内容部分*/
        .banner-container{width: 1250px;position: absolute;z-index: 8888;top: 0;left: 50%;margin-left: -625px;height: 0;}
        .banner-container .banner-items{width: 200px;height: 396px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
        .banner-container .banner-items .title{background-color: #333;color: #FFF;font-size: 15px;padding: 10px 10px;}
        .banner-container .banner-items .inner-content{background-color: #4A4A4A;padding:  18px 10px;font-size: 13px;color: #FFF;}
        .banner-container .banner-items .inner-content .btn{width: 100%;border: none;color: #FFF;padding: 7px 0;}
        .banner-container .banner-items .inner-content .btn:focus{outline: 0;}
        .banner-container .banner-items .inner-content .name{font-size: 30px;border-top: 1px solid #545454;color: #666;margin: 15px 0 10px;}
        .banner-container .banner-items .inner-content .col-40{text-align: center;}
        .banner-container .banner-items .inner-content .col-60 p{margin: 2px 0;}
        .banner-container .banner-items .inner-content .col-60 p:last-child{border: 1px solid #D1915A;display: inline-block;color: #D1915A;}
        .banner-container .banner-items .item-bottom{padding: 10px 10px 0;font-size: 13px;}
        .banner-container .banner-items .item-bottom p{margin: 5px 0;}
        .banner-container .item1{background-color: #3B3B3B;float: left;width: 190px;}
        .banner-container .item2{float: right;background-color: #FFF;padding: 10px;position: relative;}
        .banner-container .item2 .item2-title{font-size: 15px;color: #3e3e3e;margin-bottom: 10px;}
        .banner-container .item2 .item2-title i{font-size: 20px;color: #999;margin-right: 10px;}
        .banner-container .item2 .sign-in{;outline:0;position:relative;background-color: #FF7200;color: #FFF;border: none;border-radius: 3px;height: 35px;display: block;width: 100%;font-size: 15px;margin :10px auto;}
        .banner-container .item2 .sign-in i{margin-right: 10px;color: #FFF;font-size: 18px;}
         .banner-container .item2 .sign-in .exp{position:absolute;display:none;opacity:0;left:70%;outline:0;top:20px;font-size:16px;font-weight:600;}
        .banner-container .item2 .item2-last-word{font-size: 13px;color: #989898;}

        /*商品列表*/
        .list-content{width: 1250px;margin: 0 auto;}
