.navigation .navigation-left .left-chunk3 ul .L-index:after{content: "";display: block;width: 0px;height: 0px;border: none;}
.category *{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.category-title{width: 100%;background-color: #FFF;height: 58px;line-height: 58px;padding-left: 20px;margin-bottom: 18px;font-weight: 600;color: #525354;}
.category-content{background-color: #FFF;border: 1px solid #ddd;padding: 20px;min-height: 485px;position: relative;color: #666;}
.category-content .paging{text-align: right;position: absolute;bottom: 28px;right: 20px;}
.category-content .paging button{height: 22px;margin-right: 2px;font-size: 12px;line-height: 15.5px;color: #999}
.cate-content-title{margin-bottom: 5px;}
.cate-content-title .cate-nav-left{float: left}
.cate-content-title .cate-nav-right li,.cate-content-title .cate-nav-left li{margin-right: 40px;}
.cate-content-title .cate-nav-left li:last-child{margin-right: 0}
.cate-content-title .cate-nav-right li:last-child{margin-right: 0}
.cate-content-title .cate-nav-right{float: right}
.category-content-block .bimg li{width: 250px;height:135px;margin-right: 21px;margin-bottom:20px;background: url("../../commons/images/BG-TIKETY.png") no-repeat;background-size: 100% 100%;color: #FFF;padding: 14px;float: left}
.category-content-block .bimg li .item-word{font-size: 15px;margin-bottom: 30px;line-height: 33px;}
.category-content-block .bimg li .item-word .num{font-size: 30px;}
.category-content-block .bimg li .item-word .fw{float: right;font-weight: 600;position: relative;top: 5px;}
.category-content-block .bimg li .item-des{font-size: 13px;}
.cate-nav{margin-bottom: 15px;}
.cate-nav li{margin-right: 15px;display: inline-block;font-size: 14px;cursor: pointer;}
.cate-nav .active{color: #E43a3d;padding-bottom: 1px;border-bottom: 1px solid #E43a3d;font-weight: 600;}
.btn-group .btn-default{border: 1px solid #ddd;margin-right: 10px;color: #525354;border-radius: 3px;background-color: #FFF;outline: none;font-family: "微软雅黑", "黑体", arial;}