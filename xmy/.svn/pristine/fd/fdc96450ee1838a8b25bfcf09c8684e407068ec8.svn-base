@CHARSET "UTF-8";
        .content{bottom: 0;}
        .head-top{background-color: #348811;color: #fff;font-size: 0.7rem;position: relative;padding: 0 10px;padding-top: 1rem;text-align: center;padding-bottom: 0.75rem;}
        .head-top .head-img{height: 4rem;width: 4rem;margin:0 auto;}
        .head-top .head-img img{width: 100%;height: 100%;}
        .head-top .head-text{margin-top: 0.5rem;}
        .head-top .head-text .b{font-size: 0.8rem;}
        .list{margin:0;font-size: 0.7rem;color: #444;}
        .list .weui-cell{padding: 15px 10px;}
        .list .weui-cell .green{color: #348811;}
        .chunk{margin-top: 4rem;margin-bottom: 0.5rem;}
        .chunk .weui-flex__item{padding: 0 10px;}
        .chunk .weui-flex__item button{width: 100%;border:0;color: #fff;font-size: 0.7rem;background-color: #348811;height: 2rem;border-radius: 3px;}
        .chunk .weui-flex__item.orange button{background-color: #FB9107;}