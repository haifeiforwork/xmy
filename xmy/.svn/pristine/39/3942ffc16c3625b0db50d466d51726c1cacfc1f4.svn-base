package com.zfj.xmy.order.persistence.app.pojo.dto;

import java.util.List;

import com.zfj.xmy.common.persistence.pojo.AdImage;

/**
 * App广告位置输出
 * @Description 
 * @Author liuw
 * @Date 2017年8月15日上午9:45:47
 */
public class AppAdInfoOutDto {

	//广告名称
	private String name;
	//展示方式
	private Integer show_ways;
	//返回的广告图片集合
	private List<AdImage> adImages;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getShow_ways() {
		return show_ways;
	}
	public void setShow_ways(Integer show_ways) {
		this.show_ways = show_ways;
	}
	public List<AdImage> getAdImages() {
		return adImages;
	}
	public void setAdImages(List<AdImage> adImages) {
		this.adImages = adImages;
	}
	
}
