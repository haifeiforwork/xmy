package com.zfj.xmy.activity.persistence.common.pojo.dto;

import java.util.List;

import com.zfj.xmy.common.persistence.pojo.AdImage;
import com.zfj.xmy.common.persistence.pojo.Goods;
import com.zfj.xmy.common.persistence.pojo.app.AppGoodsCondense;

/**
 * 货柜返回扩展类
 * @Description 
 * @Author liuw
 * @Date 2017年8月15日下午2:34:04
 */
public class ContainerOutDto {
	//图标地址
	private String iconImg;
	//背景地址
	private String bgImg;
	//口号
	private String slogan;
	//货柜名字
	private String name;
	//此货柜绑定的分类id
	private Long categoryId;
	//货柜的广告
	private List<AdImage> containerAd;
	//货柜上的商品
	private List<AppGoodsCondense> goods;
	//权重
	private Integer weight;
	public String getIconImg() {
		return iconImg;
	}
	public void setIconImg(String iconImg) {
		this.iconImg = iconImg;
	}
	public String getBgImg() {
		return bgImg;
	}
	public void setBgImg(String bgImg) {
		this.bgImg = bgImg;
	}
	public String getSlogan() {
		return slogan;
	}
	public void setSlogan(String slogan) {
		this.slogan = slogan;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Long getCategoryId() {
		return categoryId;
	}
	public void setCategoryId(Long categoryId) {
		this.categoryId = categoryId;
	}
	public List<AdImage> getContainerAd() {
		return containerAd;
	}
	public void setContainerAd(List<AdImage> containerAd) {
		this.containerAd = containerAd;
	}
	public List<AppGoodsCondense> getGoods() {
		return goods;
	}
	public void setGoods(List<AppGoodsCondense> goods) {
		this.goods = goods;
	}
	public Integer getWeight() {
		return weight;
	}
	public void setWeight(Integer weight) {
		this.weight = weight;
	}
	
}
