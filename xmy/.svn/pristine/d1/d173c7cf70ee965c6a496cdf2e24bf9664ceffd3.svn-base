package com.zfj.xmy.goods.persistence.wap.pojo.dto;

public class ShoppingGoodsUpdateDto {
	
	private Long userId;
	
	private Long goodsId;
	
	private Integer num;
	
	public ShoppingGoodsUpdateDto() {}

	/**
	 * @param userId
	 * @param goodsId
	 * @param num
	 */
	public ShoppingGoodsUpdateDto(Long userId, Long goodsId, Integer num) {
		super();
		this.userId = userId;
		this.goodsId = goodsId;
		this.num = num;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public Long getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}

	public Integer getNum() {
		return num;
	}

	public void setNum(Integer num) {
		this.num = num;
	}
	
}
