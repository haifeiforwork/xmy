header .header-top{border-bottom: 1px solid #C3C3C3;}
.content{bottom: 0;}
.list .weui-cells{margin: 0;}
.list .weui-cells .weui-cell{border-bottom: 1px solid #E5E5E5;padding: 13px 10px;}
.list .weui-cells .weui-cell .weui-cell__hd{font-size: 0.7rem;color: #444;}
.list .weui-cells .weui-cell .weui-cell__hd .weui-label{width: 75px;}
.list .weui-cells .weui-cell .weui-cell__bd input{font-size: 0.7rem;color: #444;}
.list .weui-cells .weui-cell .weui-cell__ft{font-size: 0.7rem;color: #444;}
.list .weui-cells .weui-cell .weui-cell__ft img{width: 2rem;height: 2rem;}
.list .weui-cells .weui-cell .weui-cell__bd{font-size: 0.7rem;color: #444;}
.list .weui-cells .text{font-size: 0.7rem;color: #444;padding: 13px 10px;min-height: 5rem;}
.list .delete-address{font-size: 0.7rem;color: #F42D30;padding: 13px 10px;background-color: #fff;margin-top: 0.5rem;}
.weui-cells:after, .weui-cells:before, .weui-cell:before{display: none;}
