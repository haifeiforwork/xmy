        .login .scaning-login .img-code i{font-size: 47px;color: #358812;}
        .login .tips-title{height: 18px;  line-height: 18px;  font-size: 16px;  color: #3c3c3c;  margin-top: 9px;  padding-bottom: 8px;  font-weight: 700;padding-left: 30px;}
        .qrcode-img{margin-top: 27px;}
        .qrcode-img img{display: block;margin: 0 auto;}
        .qrcode-word{ overflow: hidden;  width: 148px;  margin: 30px auto 0;}
        .qrcode-word i{ float: left;  color: #358812;  font-size: 34px;  line-height: 36px;  margin-right: 10px;}
        .qrcode-word p{width: 98px;  line-height: 18px;  color: #6c6c6c;  font-size: 12px;  float: left;}
