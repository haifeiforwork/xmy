.navigation .navigation-left .left-chunk3 ul .L-index:after{content: "";display: block;width: 0px;height: 0px;border: none;}
.navigation .navigation-left .left-chunk2 .logo-near{color: #FFF;margin-top: 15px;}
.category *{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.category .text-center{text-align: center;}
.category .price{color: #EF1722;}
.navigation .navigation-right .L-search{margin-right: 0;width: 338px;}
.navigation .navigation-right .L-search input{width: 268px;}
.navigation .navigation-right .L-search button{float: right;}
.houese-top{border-bottom: 2px solid #b5b5b5;padding:7px 0 7px 30px;}
.houese-top .house-list-top{width: 60%;float: left;height: 30px;line-height: 30px;font-size: 15px;}
.houese-top .house-list-top li{display: inline-block;margin-right: 60px;}
.houese-top .house-list-top li.active{color: #358812;}
.houese-top .btn-input{width: 40%;float: right;text-align: right}
.houese-top .btn-input .line-btn-group{margin-right: 20px;}
.houese-top .btn-input .line-btn-group,.houese-top .btn-input .input-ad-btn{display: inline-block;position: relative}
.houese-top .btn-input .line-btn-group .btn-default{background-color: #FFF;border: 1px solid #ddd;padding: 5px 11px;}
.houese-top .btn-input .input-ad-btn input{width: 160px;border: 1px solid #ddd;height: 28px;outline: 0;padding-left: 10px;}
.houese-top .btn-input .input-ad-btn button{position: absolute;background-color: #f5f5f5;right: 0;top: 0;border: 1px solid #ddd;height: 28px;}
.houese-top .btn-input .input-ad-btn button i{font-size: 13px;color: #999;}
.category-title{width: 100%;background-color: #FFF;height: 58px;line-height: 58px;padding-left: 25px;margin-bottom: 18px;font-weight: 600;}
.houese-top-title{font-weight: normal;color: #585858;}
.houese-top-title .houese-list,.houese-content{font-size: 13px;}
.houese-top-title .houese-list li{display: inline-block;margin-right: 27px;line-height: 1;cursor: pointer;}
.houese-top-title .houese-list li:last-child{margin-right: 0;}
.houese-top-title .houese-list li.active{color: #359812}
.houese-top-title .houese-list li.add-bor-l{border-left: 5px solid #359812;padding:2px 0 2px 7px;}
.houese-content .house-item{width: 226px;margin-right: 30px;float: left;margin-bottom: 20px;}
.houese-content .house-item .img-wrap{background-color: #FFF;padding: 15px;width: 100%;height: 214px;border: 1px solid #B5B5B5;}
.houese-content .house-item .img-wrap img{width: 100%;height: 100%;}
.houese-content .house-item p{margin-top: 10px;}
.houese-content .house-item .house-word{display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;word-break: break-all;height: 34px; color: #525354;}
.houese-content .house-item .price{font-weight: 600;}
.one-paging{margin-top: 60px;height: 40px;line-height: 40px;text-align: right;font-size: 13px;}
.one-paging button{background-color: #FFF;color: #999;border: 1px solid #ddd;padding: 9px 11px;border-radius: 2px;}
.one-paging button.pre{background-color: #F5F5F5;margin-right: 18px;}
.one-paging .paging-all,.jump-paging{display: inline-block;}
.one-paging .paging-all a{display: inline-block;color: #999;border: 1px solid #ddd;padding: 0 15px;text-decoration: none;height: 37px;line-height: 36px;background-color: #FFF;margin-right: 10px;}
.one-paging .paging-all a:last-child{margin-right: 0;}
.one-paging .paging-all a.active{background-color: #359812;color: #FFF;}
.one-paging .jump-paging{margin-left: 10px;}
.one-paging .jump-paging input{height: 37px;width: 42px;border: 1px solid #ddd;border-radius: 2px;text-align: center}
.one-paging .jump-paging button{padding: 9px 18px;}