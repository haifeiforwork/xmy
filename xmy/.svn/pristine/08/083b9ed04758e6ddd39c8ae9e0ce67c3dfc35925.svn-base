.float-icon .icon-item3{background-image: url("../../commons/images/kjzq.png")}
.float-icon .icon-item4{background-image: url("../../commons/images/ggfs.png")}
.float-icon .icon-item5{background-image: url("../../commons/images/xxls.png")}
.float-icon .icon-item6{background-image: url("../../commons/images/lyfs.png")}
.float-icon .icon-item7{background-image: url("../../commons/images/shry.png")}
.float-icon .icon-item8{background-image: url("../../commons/images/jsyl.png")}
.float-icon .icon-item9{background-position: -18px -162px;}
.float-icon .icon-item{background-repeat: no-repeat;width: 20px;height: 20px;display: block;background-size:100%;}
.float-icon .icon-word{position: absolute;font-size: 12px;color: #FFF;left: -7px;top: -9px;display: none;}

	@keyframes fadeIn {
		0% {
			opacity: 0;
			top : -50px;
		}
		100% {
			opacity: 1;
			top : 0;
		}
	}
	@keyframes fadeLeft {
		0% {
			opacity: 0;
            margin: -160px 0 0 -700px;
		}
		100% {
			opacity: 1;
            margin: -160px 0 0 -680px;
		}
	}
.fadein{
		-webkit-animation: fadeIn 0.7s ease both;
		-o-animation: fadeIn 0.7s ease both;
		animation: fadeIn 0.7s ease both;}
.float-icon{
	-webkit-animation: fadeLeft 0.7s ease both;
	-o-animation: fadeLeft 0.7s ease both;
	animation: fadeLeft 0.7s ease both;}

	.sing-bg{background: url("../../commons/images/jxsp_top.png") right top no-repeat;height: 35px;background-size: 100%;}