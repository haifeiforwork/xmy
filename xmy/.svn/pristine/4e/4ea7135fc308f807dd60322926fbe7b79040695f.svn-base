.content{top: 0;bottom: 0;}
.container-white{background-color: #FFF;width: 100%;height: 100%;}
.container .logo-area{width: 50%;margin: 0 auto 1.5rem;}
.container .logo-area img{width: 100%;}
.container .self-cell{font-size: 0.7rem;}
.container .theme-color{color: #358812;}
.container .text-right{text-align: right;}
.self-cell .weui-cell{padding: 10px;}
.self-cell .weui-input::-webkit-input-placeholder { /* WebKit browsers */ color:#999;}
.self-cell .weui-input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */color:#999;}
.self-cell .weui-input::-moz-placeholder { /* Mozilla Firefox 19+ */color:#999;}
.self-cell .weui-input:-ms-input-placeholder { /* Internet Explorer 10+ */color:#999;}
.self-cell .hd-img{width: 25px;height: 25px;margin-right: 10px;}
.self-cell .js-bd{width: 30px;height: 26px;}
.self-cell .js-bd1{background: url("../images/btn-login-off@2x.png") no-repeat;background-position: center;background-size: 100% 100%}
.self-cell .js-bd2{background: url("../images/btn-login-on@2x.png") no-repeat;background-position: center;background-size: 100% 100%}
.self-btn-area .weui-btn_primary{background-color: #358812;font-size: 0.75rem;line-height:2.2rem;}
.self-btn-area{margin: 1.5rem 10px 0.3rem;}
.container-wrap{font-size: 0.7rem;padding: 5px 10px;overflow: hidden}
.container-wrap .reg{float: left;margin-left: 0.7rem;}
.container-wrap .reg-right{float: right;margin-right: 0.7rem;}
.self-flex{font-size: 0.7rem;color:#666;}
.self-flex .login-icon{width: 50px;height: 50px;}
.self-footer .weui-footer__link{font-size: 0.6rem;}