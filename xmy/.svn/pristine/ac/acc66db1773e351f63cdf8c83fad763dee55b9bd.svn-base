package com.zfj.xmy.freight.vo;

import com.zfj.xmy.common.persistence.pojo.Goods;

public class FreightGoods {

	/**
	 * 商品
	 */
	private Goods goods;
	
	/**
	 * 商品数量
	 */
	private int num;

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public Goods getGoods() {
		return goods;
	}

	public void setGoods(Goods goods) {
		this.goods = goods;
	}
	
	
}
